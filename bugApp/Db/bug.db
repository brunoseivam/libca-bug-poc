    field(PINI, "YES")
    field(PREC, "1")
}

record(ai, "$(P)SUM_RD") {
    field(DTYP, "Python Device")
    field(INP,  "@devsup.ptable bug get s")
    field(PINI, "YES")
    field(PREC, "1")
